ends

defstate validatetarget


ifn target -1 
{
	geta[target].statnum TEMP8
	ife TEMP8 1024 set target -1

	state checksight
	
	ifn target -1
	{
		geta[target].x mx
		geta[target].y my
	}
	geta[].x x
	geta[].y y
	sub mx x
	sub my y
	getangle ANGVAR mx my
	seta[].ang ANGVAR

	state visibilitycheck
}
else break

ends

