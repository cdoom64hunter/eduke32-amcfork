				state spawn_gunsmoke_z
				set gunsmoke_z 344
				espawn 16115
				state spawn_muzzleflash
				}
			ifrnd 32 ai AIWASP_DRONEPULLBACK
			ifg ALLY_MAG 10 ai AIWASP_DRONEPULLBACK
		}
		resetactioncount
	}
	else ife target -1 ai AIWASP_DRONEPULLBACK

ends

