	ifand AIRDROP_LEVELS 512 ife INFO_SEL 14 rotatesprite GUI_POS_X GUI_POS_Y 20000 0 14737 0 0 0 0 0 xdim ydim // Life Tower

	ifl GUI_POS_X_TEMP 15
	 ifg GUI_POS_X_TEMP -15
	   ifl GUI_POS_Y_TEMP 10
		ifg GUI_POS_Y_TEMP -10
		{
		set MOUSE_HINT 3090 // refers to quote in user.con
		ifand AIRDROP_LEVELS 2 ife INFO_SEL 2 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 4 ife INFO_SEL 3 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 8 ife INFO_SEL 4 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 16 ife INFO_SEL 6 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 32 ife INFO_SEL 10 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 64 ife INFO_SEL 11 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 128 ife INFO_SEL 12 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 256 ife INFO_SEL 13 set MOUSE_HINT 3093
		ifand AIRDROP_LEVELS 512 ife INFO_SEL 14 set MOUSE_HINT 3093
		sub MOUSE_HINT_TIMER 2
		  ifand BITS_PRESS 4
		   ife BUTTON_COUNTER 0
			{
			ife INFO_SEL 2 // Megabase
				{ 
				ifand AIRDROP_LEVELS 2 { soundonce INTERFACE_1 xorvar AIRDROP_LEVELS 2 savegamevar AIRDROP_LEVELS } 
				else { soundonce INTERFACE_2 xorvar AIRDROP_LEVELS 2 savegamevar AIRDROP_LEVELS } 
