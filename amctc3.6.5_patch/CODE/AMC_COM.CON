				ifand AIRDROP_LEVELS 4 { soundonce INTERFACE_1 xorvar AIRDROP_LEVELS 4 savegamevar AIRDROP_LEVELS } 
				else { soundonce INTERFACE_2 xorvar AIRDROP_LEVELS 4 savegamevar AIRDROP_LEVELS } 
				}
			ife INFO_SEL 4 // Snowbase
				{ 
				ifand AIRDROP_LEVELS 8 { soundonce INTERFACE_1 xorvar AIRDROP_LEVELS 8 savegamevar AIRDROP_LEVELS } 
				else { soundonce INTERFACE_2 xorvar AIRDROP_LEVELS 8 savegamevar AIRDROP_LEVELS } 
