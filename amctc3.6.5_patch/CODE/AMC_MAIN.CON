defstate SPRITE_FADE
		ife alpha 0
		{
		geta[].cstat TEMP
		andvar TEMP 0xfdfe
		seta[].cstat TEMP
		}
		else
		{
			geta[].cstat TEMP
			orvar TEMP 2
			seta[].cstat TEMP

			// Assume blending tables [1 .. 128] are installed, like generated by
			// shadexfog.lua:create_128_trans(1).
			ifg alpha 128
			{
				set TEMP 257
				sub TEMP alpha
				set alpha TEMP

				// Set "reverse translucent" cstat bit
				geta[].cstat TEMP
				orvar TEMP 512
				seta[].cstat TEMP
			}
			else
			{
				// Clear cstat bit 512
				geta[].cstat TEMP
				andvar TEMP 0xfdff
				seta[].cstat TEMP
			}
			seta[].blend alpha
		}
ends

