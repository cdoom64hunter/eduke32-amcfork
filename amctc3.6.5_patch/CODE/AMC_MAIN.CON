					gettspr[].tsprcstat TSPRITE_TEMP
					orvar TSPRITE_TEMP 512
					settspr[].tsprcstat TSPRITE_TEMP
				}
				else
				{
					// Clear cstat bit 512
					gettspr[].tsprcstat TSPRITE_TEMP
					andvar TSPRITE_TEMP 0xfdff
					settspr[].tsprcstat TSPRITE_TEMP
				}

				settspr[].tsprblend alpha
			}
		}
		
