defstate reverse_shade
  set WEAP_DISP_TEMP5 shade
  ifg WEAP_DISP_TEMP5 30 set WEAP_DISP_TEMP5 30
  ifl WEAP_DISP_TEMP5 0 set WEAP_DISP_TEMP5 0
